@page

@model FleetManager.Pages.Asignaciones.DeleteModel
@{
    ViewData["Title"] = "Eliminar Asignación";
}

<h1>Eliminar Asignación</h1>

<p>¿Estás seguro que deseas eliminar esta asignación?</p>

