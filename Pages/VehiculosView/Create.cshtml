@page
@model FleetManager.Pages.VehiculosView.CreateModel
@{
    ViewData["Title"] = "Agregar Vehículo";
}

<h1>Agregar Vehículo</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Vehiculo.Placa" class="control-label"></label>
        <input asp-for="Vehiculo.Placa" class="form-control" />
        <span asp-validation-for="Vehiculo.Placa" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Vehiculo.Marca" class="control-label"></label>
        <input asp-for="Vehiculo.Marca" class="form-control" />
        <span asp-validation-for="Vehiculo.Marca" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Vehiculo.Modelo" class="control-label"></label>
        <input asp-for="Vehiculo.Modelo" class="form-control" />
        <span asp-validation-for="Vehiculo.Modelo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Vehiculo.Año" class="control-label"></label>
        <input asp-for="Vehiculo.Año" class="form-control" />
        <span asp-validation-for="Vehiculo.Año" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Vehiculo.Kilometraje" class="control-label"></label>
        <input asp-for="Vehiculo.Kilometraje" class="form-control" />
        <span asp-validation-for="Vehiculo.Kilometraje" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
    <a asp-page="Index" class="btn btn-secondary">Cancelar</a>
</form>

