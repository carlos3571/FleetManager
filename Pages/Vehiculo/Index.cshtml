@page
@model IndexModel

<h1>Vehículos</h1>

<table>
    <thead>
        <tr>
            <th>Placa</th>
            <th>Marca</th>
            <th>Modelo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var vehiculo in Model.Vehiculos)
        {
            <tr>
                <td>@vehiculo.Placa</td>
                <td>@vehiculo.Marca</td>
                <td>@vehiculo.Modelo</td>
            </tr>
        }
    </tbody>
</table>

